function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               999
    set -U fish_color_param                 white
    set -U fish_color_redirection           white
    set -U fish_color_comment               666
    set -U fish_color_error                 f00
    set -U fish_color_escape                999
    set -U fish_color_operator              999
    set -U fish_color_end                   f00
    set -U fish_color_quote                 f00
    set -U fish_color_autosuggestion        666
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   999
    set -U fish_color_cwd_root              999
    set -U fish_color_match                 999
    set -U fish_color_search_match          --background=white
    set -U fish_color_selection             --background=white
    set -U fish_pager_color_prefix          cyan
    set -U fish_pager_color_completion      white
    set -U fish_pager_color_description     cyan
    set -U fish_pager_color_progress        cyan
    set -U fish_color_history_current       999
end
